<h1>users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>

<%= @users.each do |user| %>
  <p>
  	<%= user.first_name %> 
  	<%= user.last_name %>  
    <%= link_to "Delete This User", user_path(user), method: :delete, data: {confirm: "Are you sure you want to delete this user?"} %> 
  </p>
<% end %>





